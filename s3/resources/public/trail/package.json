{
  "name": "ontrail",
  "version": "1.0.0",
  "dependencies": {
    "browserify-shim": "~3.2.0",
    "lodash": "2.4.1",
    "jquery": "2.1.1",
    "rx": "2.3.14",
    "rx-jquery": "1.1.7 ",
    "medium-editor": "1.9.8",
    "moment": "2.8.3",
    "cookie-cutter": "0.1.1"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "browserify-shim": "^3.8.0"
  },
  "engines": {
  },
  "scripts": {
    "start": "watchify js/main.js -d -o js/main-bundle.js -v"
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/jrosti/ontrail"
  },
  "browser": {
    "livestamp": "./js/lib/jquery/jquery.livestamp.js",
    "medium-editor-insert-plugin": "./js/lib/medium-editor/medium-editor-insert-plugin.min.js",
    "medium-editor-insert-images": "./js/lib/medium-editor/medium-editor-insert-images.min.js"
  },
  "browserify-shim": {
    "livestamp": {
      "depends": "jquery"
    },
    "medium-editor-insert-plugin": {
      "depends": ["jquery", "medium-editor"]
    },
    "medium-editor-insert-images": {
      "depends": ["jquery", "medium-editor", "medium-editor-insert-plugin"]
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  }
}
